{
  "collectCoverage": true,
  "collectCoverageFrom": [
    "src/**/*.{js,jsx}"
  ],
  "snapshotSerializers": [
    "enzyme-to-json/serializer"
  ],
  "setupFiles": [
    "<rootDir>/src/tests/setupTests.js",
    "jest-localstorage-mock"
  ],
  "moduleNameMapper": {
    "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/tests/components/__mocks__/fileMock.js",
    "\\.(css|scss)$": "<rootDir>/src/tests/components/__mocks__/fileMock.js"
  }
}
